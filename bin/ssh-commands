# What ruby do I have installed.
which ruby

# .ssh
cd ~/.ssh
chmod 600 * 
chmod 644 *.pub

# See what is set.
echo $HOST
echo $HOSTFILE
echo $HOSTNAME
echo $HOSTTYPE
echo $INPUTRC
echo $MACHTYPE
echo $UID

